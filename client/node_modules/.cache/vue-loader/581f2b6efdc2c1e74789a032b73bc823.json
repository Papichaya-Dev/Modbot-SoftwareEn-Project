t/src/views/register.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"row text-center\">\r\n      <div class=\"card mx-auto\">\r\n        <div class=\"card-header text-white bg-dark\">\r\n          <h4>Register</h4>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <form @submit.prevent=\"registerUser\">\r\n            <div class=\"form-group\">\r\n              <label for=\"username\">Username</label>\r\n              <input\r\n                id=\"username\"\r\n                type=\"text\"\r\n                placeholder=\"Username\"\r\n                name=\"username\"\r\n                v-model=\"username\"\r\n                class=\"form-control\"\r\n              />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"name\">Name</label>\r\n              <input\r\n                id=\"name\"\r\n                type=\"text\"\r\n                placeholder=\"Name\"\r\n                name=\"name\"\r\n                v-model=\"name\"\r\n                class=\"form-control\"\r\n              />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"email\">Email</label>\r\n              <input\r\n                id=\"email\"\r\n                type=\"text\"\r\n                placeholder=\"Email\"\r\n                name=\"email\"\r\n                v-model=\"email\"\r\n                class=\"form-control\"\r\n              />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"password\">Password</label>\r\n              <input\r\n                type=\"password\"\r\n                class=\"form-control\"\r\n                placeholder=\"Password\"\r\n                name=\"password\"\r\n                id=\"password\"\r\n                v-model=\"password\"\r\n              />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"confirm_password\">Confirm Password</label>\r\n              <input\r\n                type=\"password\"\r\n                class=\"form-control\"\r\n                placeholder=\"Confirm Password\"\r\n                name=\"confirm_password\"\r\n                id=\"confirm_password\"\r\n                v-model=\"confirm_password\"\r\n              />\r\n            </div>\r\n            <button class=\"btn btn-dark\">Register</button>\r\n            &nbsp;&nbsp;&nbsp;&nbsp;\r\n            <router-link to=\"/login\" class=\"card-link text-dark\"\r\n              >Already have an account?</router-link\r\n            >\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from \"vuex\";\r\nexport default {\r\n  name: \"Register\",\r\n  created() {\r\n    document.title = \"ModBot | \" + this.$options.name;\r\n  },\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\",\r\n      confirm_password: \"\",\r\n      name: \"\",\r\n      email: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions([\"register\"]),\r\n    registerUser() {\r\n      let user = {\r\n        username: this.username,\r\n        password: this.password,\r\n        confirm_password: this.confirm_password,\r\n        email: this.email,\r\n        name: this.name,\r\n      };\r\n      this.register(user).then((res) => {\r\n        if (res.data.success) {\r\n          this.$router.push(\"login\");\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card {\r\n  width: 40%;\r\n  border-radius: 0;\r\n}\r\n.btn {\r\n  border-radius: 0;\r\n}\r\n.form-control {\r\n  border-radius: 0;\r\n}\r\n</style>\r\n"]}]}